        removeFromCart: function() {
            var $object = this;
            $('#miniCart').on('click', '.product-item_remove', function(e){
                e.preventDefault();
                var $id = $(this).data('product-id');
                $.ajax({
                    url: '/cart/change.js',
                    type: 'POST',
                    data: {
                        id: $id,
                        quantity: 0
                    },
                    dataType: 'json',
                    beforeSend: function() {

                    },
                    success: function(cart) {
                        $object.updateCartCanvas();
                    }
                });
