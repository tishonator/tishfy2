        },
        updateCartCanvas: function(cart) {
            var $object = this;
            if (cart == undefined) {
                $.ajax({
                    url: '/cart.js',
                    type: 'GET',
                    dataType: 'json',
                    beforeSend: function() {

                    },
                    success: function(cart) {
                        $object.createCartCanvasItem(cart);
                        $('.header-cart .item-counter').text(cart.item_count);
                    }
                });
            }
            else {
                $object.createCartCanvasItem(cart);
            }
        },
        createCartCanvasItem: function(cart) {
            var emptyNotification = '<p>No product in the cart</p>';
            var itemTemplate = '<li class="minicart-product"><a class="product-item_remove" href="javascript:void(0)" data-product-id="{variant_id}"><i class="icon-cross2"></i></a><a href="{url}" class="product-item_img"><img src="{image}" /></a><div class="product-item_content"><a class="product-item_title" href="{{ line_item.url }}">{title}</a><label>Qty : <span>{quantity} Ã— </span></label><label class="product-item_quantity">{price}</label></div></li>';
            if (cart.item_count == 0) {
                $('#miniCart .minicart-content').html(emptyNotification);
                $('#miniCart .minicart-btn_area, #miniCart .minicart-item_total').addClass('d-none');
            }
            else {
                var itemList = '';
                for (var i = 0; i < cart.items.length; i++) {
                    var item = itemTemplate;
                    item = item.replace(/\{url\}/g, cart.items[i].url);
                    item = item.replace(/\{title\}/g, cart.items[i].title);
                    item = item.replace(/\{quantity\}/g, cart.items[i].quantity);
                    item = item.replace(/\{image\}/g, cart.items[i].image, 'small');
                    item = item.replace(/\{price\}/g, Shopify.formatMoney(cart.items[i].price, moneyFormat));
                    item = item.replace(/\{variant_id\}/g, cart.items[i].variant_id);
                    itemList += item;
                }
                $('#miniCart .minicart-content').html('<ul class="minicart-list">' + itemList + '</ul>');
                $('#miniCart .minicart-item_total .amount').text(Shopify.formatMoney(cart.total_price, moneyFormat));
                $('#miniCart .minicart-btn_area, #miniCart .minicart-item_total').removeClass('d-none');
            }
        },
