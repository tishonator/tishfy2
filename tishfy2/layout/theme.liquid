        addToCartFromProductPage: function () {
            var $object = this;
            $('body').on('click', '.cart_add_to_cart_button', function(e){
                e.preventDefault();
                var $this   = $(this);
                $.ajax({
                    url: '/cart/add.js',
                    type: 'POST',
                    data: $this.parents("form#AddToCartForm").serialize(),
                    dataType: "json",
                    beforeSend: function(){
