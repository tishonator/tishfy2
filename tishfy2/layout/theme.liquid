  var TishonatorCategory = {
        init: function () {            
            this.initQueryParams();
            this.changeCategory();
            this.changeFilter();
            this.clearFilter();
            this.clearAllFilter();
            this.changeSortingOrder();
            this.changePagination();
        },
        initQueryParams: function () {
            Shopify.queryParams = {};
            if (location.search.length) {
                for (var aKeyValue, i = 0, aCouples = location.search.substr(1).split('&'); i < aCouples.length; i++) {
                    aKeyValue = aCouples[i].split('=');
                    if (aKeyValue.length > 1) {
                        Shopify.queryParams[decodeURIComponent(aKeyValue[0])] = decodeURIComponent(aKeyValue[1]);
                    }
                }
            }
        },
        changeCategory: function () {
            var _this = this;
            $('.categories-list').on("click", "a", function(e) {                
                e.preventDefault();
                var $this = $(this);
                delete Shopify.queryParams.page;
                $('.categories-list li').removeClass('active');
                $this.parent().addClass('active');
                $this.closest('.children').parent().addClass('active');

                var $url = $this.attr('href');
                _this.getContent($url);
            });
        },
        changeFilter: function () {
            var _this = this;
            $(".product-sidebar").on("change", ".widget-product-filter input", function() {
                delete Shopify.queryParams.page;
                _this.addQueryConstraint();
                _this.getContent();
            });
        },
        clearFilter: function () {
            var _this = this;
            $(".product-sidebar").on("click", ".clear-filter", function(e) {
                e.preventDefault();
                var $this = $(this);
                var tag = $this.data("filter");
                $(".product-sidebar input[value='" + tag + "']").prop('checked', false);
                delete Shopify.queryParams.page;

                _this.addQueryConstraint();
                _this.getContent();
            });
        },
        clearAllFilter: function () {
            var _this = this;
            $(".product-sidebar").on("click", ".clear-all-filter", function(e) {
                e.preventDefault();
                $(".product-sidebar input").prop('checked', false);
                delete Shopify.queryParams.page;

                _this.addQueryConstraint();
                _this.getContent();
            });
        },
        changeSortingOrder: function () {
            var _this = this;
            if (typeof sortOrder != "undefined") {
                $("#SortBy").val(sortOrder);
            }
            $(".product-wrapper").on("change", "#SortBy", function() {
                var $this = $(this);
                delete Shopify.queryParams.page;
                Shopify.queryParams.sort_by = jQuery(this).val();
                _this.getContent();
            });
        },
        changePagination: function () {
            var _this = this;
            $(".products").on("click", "ul.pagination a", function(e) {
                e.preventDefault();
                var $this = $(this);
                Shopify.queryParams.page = $this.data('page');
                _this.getContent();
            }); 
        },
        getContent: function (url) {
            var _this = this;
            $.ajax({
                type: "GET",
                url: _this.processUrl(url),
                beforeSend: function() {
                    $('body').addClass('filter-loading');
                },
                success: function(data) {
                    _this.buildContent(data);
                    //t TishonatorProduct.getRating();
                    $('body').removeClass('filter-loading');
                    $('body').trigger('is-filter-success');
                }
            });
        },
        buildContent: function (data) {
            var dataObject = $(data);
            $("head title").html(data.substring(data.indexOf("<title>")+7,data.indexOf("</title>")));
            $(".site-main .products").html(dataObject.find(".site-main .products").html());
            $(".filter-container").html(dataObject.find(".filter-container").html());
            var sort = !Shopify.queryParams.sort_by ? $("#SortBy").val(sortOrder) : $("#SortBy").val(Shopify.queryParams.sort_by);
            $(".switch-layout a").removeClass("active");
            var layout = !Shopify.queryParams.view ? $(".switch-layout a[data-action='grid']").addClass("active") : $(".switch-layout a[data-action='" + Shopify.queryParams.view + "']").addClass("active");
            $('.widget-product-filter [data-toggle="tooltip"]').tooltip();
            $("body, html").animate({scrollTop : $('body').offset().top - 50}, 500);
        },
        processUrl: function (url) {
            var _this = this;
            var query = $.param(Shopify.queryParams).replace(/%2B/g, '+');
            query = query != "" ? "?" + query : "";
            var newUrl = url != undefined ? url + query : location.pathname + query;
            //t History.pushState({}, newUrl, newUrl);
            return newUrl;
        },        
        addQueryConstraint: function () {
            delete Shopify.queryParams.constraint;
            var constraint = [];
            $(".widget-product-filter input[type='checkbox']:checked").each(function(index, element) {
                constraint.push($(this).val());
            });
            if (constraint.length > 0) Shopify.queryParams.constraint = constraint.join("+");
        }
    }

    $(document).ready(function() {
        TishonatorCategory.init();
    });


